{
  "extends": [
    "standard-with-typescript",
    "prettier",
    "plugin:react-hooks/recommended"
  ],
  "parserOptions": {
    "project": "tsconfig.json"
  },
  "rules": {
    "react/prop-types": "off",
    "import/order": "error",
    "@typescript-eslint/restrict-template-expressions": "off"
  }
}
